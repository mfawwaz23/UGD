import java.util.*;
import java.lang.Math;

public class JavaApplication {
    private static Scanner input = new Scanner(System.in);

    public static void main(String[] args) {
        String penyakit;
        
        penyakit = input.nextLine();
        if (penyakit.equals("kritis")) {
            System.out.println("Kategori 1");
        } else {
            if (penyakit.equals("nyeri dada") || penyakit.equals("sesak napas") || penyakit.equals("patah tulang") || penyakit.equals("kejang")) {
                System.out.println("Kategori 2");
            } else {
                if (penyakit.equals("pendarahan hebat") || penyakit.equals("dehidrasi berat")) {
                    System.out.println("Kategori 3");
                } else {
                    if (penyakit.equals("keseleo") || penyakit.equals("migrain") || penyakit.equals("sakit telinga")) {
                        System.out.println("Kategori 4");
                    } else {
                        System.out.println("Kategori 5");
                    }
                }
            }
        }
    }
}
